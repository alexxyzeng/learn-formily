{
  "name": "learn-formily",
  "version": "1.0.0",
  "description": "Scaffold for umi based project",
  "scripts": {
    "start": "umi dev",
    "build": "UMI_ENV=prod umi build"
  },
  "license": "MIT",
  "husky": {
